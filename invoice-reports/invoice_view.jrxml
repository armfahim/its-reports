	<queryString>
		<![CDATA[SELECT
	*,
	CASE
		WHEN ids.term = 'FIFTEEN' THEN '15'
		WHEN ids.term = 'SEVEN' THEN '7'
		WHEN ids.term = 'TWENTYONE' THEN '21'
		WHEN ids.term = 'TWENTYEIGHT' THEN '28'
		WHEN ids.term = 'THIRTY' THEN '30'
		WHEN ids.term = 'COD' THEN 'COD'
		ELSE NULL
	END as terms
FROM
	invoice_details ids
JOIN supplier_details sd ON
	ids.supplier_id = sd.id
WHERE
	ids.id = $P{INVOICE_ID}]]>
	</queryString>
